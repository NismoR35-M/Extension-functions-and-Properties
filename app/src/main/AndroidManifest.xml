<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.extension.function.properties"
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools">

  <application
      android:allowBackup="false"
      android:icon="@mipmap/ic_launcher"
      android:label="@string/app_name"
      android:roundIcon="@mipmap/ic_launcher_round"
      android:supportsRtl="true"
      android:theme="@style/AppTheme"
      tools:ignore="GoogleAppIndexingWarning">
    <activity
        android:name="com.extension.function.properties.ui.SplashActivity"
        android:screenOrientation="portrait"
        android:theme="@style/SplashTheme">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />

        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
    <activity
        android:name="com.extension.function.properties.ui.MainActivity"
        android:screenOrientation="portrait" />
    <activity
        android:name="com.extension.function.properties.ui.RegisterActivity"
        android:label="Register"
        android:parentActivityName="com.extension.function.properties.ui.OnBoardingActivity"
        android:screenOrientation="portrait" />
    <activity
        android:name="com.extension.function.properties.ui.LoginActivity"
        android:label="Login"
        android:parentActivityName="com.extension.function.properties.ui.OnBoardingActivity"
        android:screenOrientation="portrait" />
    <activity
        android:name="com.extension.function.properties.ui.OnBoardingActivity"
        android:screenOrientation="portrait"
        android:theme="@style/OnBoardingTheme" />

  </application>

  <uses-permission android:name="android.permission.INTERNET" />

</manifest>